<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window>
      <VerticalLayout name="ylConvPanel" bordersize="2,0,0,2">
            <Control height="100" bordersize="2,0,0,0"/>
            <HorizontalLayout height="50" bordersize="2,0,0,0">
                  <Label text="加密数据" float="true" pos="100,0,200,25"/>
                  <Edit name="ylEncryptData" float="true" pos="210,0,560,25" bordersize="1" />
                  <Label text="示例: 8000,8201...以逗号分隔" float="true" pos="570,0,770,25"/>
            </HorizontalLayout>
            <HorizontalLayout height="50" bordersize="2,0,0,0">
                  <Label text="解密密钥" float="true" pos="100,0,200,25"/>
                  <Edit name="ylDecryptKey" float="true" pos="210,0,560,25" bordersize="1" />
            </HorizontalLayout>
            <HorizontalLayout height="50" bordersize="2,0,0,0">
                  <Label text="值数据" float="true" pos="100,0,200,25"/>
                  <Edit name="ylValueData" float="true" pos="210,0,560,25" bordersize="1" />
                  <Label text="示例: 8000,9000...以逗号分隔" float="true" pos="570,0,770,25"/>
            </HorizontalLayout>
            <HorizontalLayout height="25" bordersize="2,0,0,0">
                  <Label text="DGI交换" float="true" pos="100,0,200,25"/>
                  <Edit name="ylExchangeData" float="true" pos="210,0,560,25" bordersize="1" />
                  <Label text="示例: (8202,8203),(8203,8204)..." float="true" pos="570,0,800,25"/>
            </HorizontalLayout>
            <HorizontalLayout>
                  <Label text="以逗号分隔" float="true" pos="570,0,800,25"/>
            </HorizontalLayout>
      </VerticalLayout>
</Window>
