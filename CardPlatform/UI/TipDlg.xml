<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="400,200" caption="0,0,0,30" roundcorner="5,5,5,5" sizebox="4,4,4,4" mininfo="600,320">
      <Font shared="true" id="0" name="幼圆" size="12" default="true" />
      <Font shared="true" id="1" name="幼圆" size="14" default="true" />
      <Font shared="true" id="2" name="微软雅黑" size="18" />
      <Font shared="true" id="3" name="微软雅黑" size="20"/>
      <Default shared="true" name="Button" value="textcolor=&quot;#FFFFFFFF&quot; bkcolor=&quot;#FF2D2D30&quot;"/>
      <Default shared="true" name="Combo" value="bkcolor=&quot;#FF3F3F46&quot; droptype=&quot;droplist&quot;"/>
      <VerticalLayout name="root" bkcolor="#FFFFFFFF">
            <!--标题栏-->
            <HorizontalLayout name="1" height="30" bkcolor="#FF2D2D30">
                  <HorizontalLayout name="2">
                        <Control width="20"/>
                        <Label name="tipLogo" padding="0,5,5,0" bkimage="file='Icon/logo.png' source='1,4,23,20'" width="22" height="16"/>
                        <Label name="tipTitle" inset="5,0,0,0" textcolor="#FFF2F2F2" font="0"/>
                  </HorizontalLayout>
                  <HorizontalLayout>
                        <HorizontalLayout>
                        </HorizontalLayout>
                        <HorizontalLayout width="40">
                              <Button float="true" pos="0,0,30,20" name="closebtn" bordersize="0" normalimage="Icon/close_10.png" />
                        </HorizontalLayout>
                  </HorizontalLayout>
            </HorizontalLayout>
            <!--对话框内容-->
            <HorizontalLayout>
                  <Label name="lbIcon" float="true" pos="20,10,60,50" />
                  <Label name="lbContent" float="true" pos="50,70,180,120"/>
            </HorizontalLayout>
            <!--对话框按钮-->
            <HorizontalLayout>
                  <HorizontalLayout>
                  </HorizontalLayout>
                  <HorizontalLayout width="80">
                        <Button name="btnOK" float="true" />
                        <Button name="btnCancel" float="true" visiable="false" />
                  </HorizontalLayout>
            </HorizontalLayout>
      </VerticalLayout>
</Window>
