                  <Label name="lbIcon" float="true" pos="20,30,100,90" />
                  <Label name="lbContent" float="true" pos="110,40,380,70"/>
            </HorizontalLayout>
            <!--对话框按钮-->
            <HorizontalLayout height="30" bkcolor="#FF7F7F7F">
                  <HorizontalLayout>
                  </HorizontalLayout>
                  <HorizontalLayout width="150">
                        <Button name="btnOK" text="确定" float="true" pos="70,0,120,25"/>
                        <Button name="btnCancel" text="取消" float="true" pos="0,0,50,25" visiable="false" />
                  </HorizontalLayout>
            </HorizontalLayout>
            <HorizontalLayout bkcolor="#FF9E9E9E" height="30">
                  
            </HorizontalLayout>
      </VerticalLayout>
</Window>
