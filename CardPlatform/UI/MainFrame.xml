<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="1200,700" caption="0,0,0,60" roundcorner="5,5,5,5" sizebox="4,4,4,4" mininfo="600,320">
      <Font shared="true" id="0" name="幼圆" size="12" default="true" />
	<Font shared="true" id="1" name="幼圆" size="14" default="true" />
	<Font shared="true" id="2" name="微软雅黑" size="18" />	
	<Font shared="true" id="3" name="微软雅黑" size="20"/>
      <VerticalLayout name="root">
            <!--标题栏-->
            <HorizontalLayout height="30" inset="0,2,0,0">
                  <HorizontalLayout>
                        <Control width="20"/>
                        <Label name="appLogo" padding="0,5,5,0" bkimage="file='Icon/logo.png' source='1,4,23,20'" width="22" height="16"/>
                        <Label name="appTitle" inset="5,0,0,0" textcolor="#FF4D4D4D" text="金邦达检查制卡平台" font="0"/>
                  </HorizontalLayout>
                  <HorizontalLayout inset="0,2,0,0">
                        <HorizontalLayout>
                        </HorizontalLayout>
                        <HorizontalLayout width="120">
                              <Button float="true" pos="0,0,40,30" name="minbtn" bordersize="0"  normalimage="Icon/min.png" />
                              <Button float="true" pos="40,0,80,30" name="restorebtn" visible="false" bordersize="0"  normalimage="Icon/restore_10.png" />
                              <Button float="true" pos="40,0,80,30" name="maxbtn" bordersize="0"  normalimage="Icon/max_10.png" />
                              <Button float="true" pos="80,0,120,30" name="closebtn" bordersize="0" normalimage="Icon/close_10.png" />
                        </HorizontalLayout>
                  </HorizontalLayout>
		</HorizontalLayout>
            <!--菜单栏-->
            <HorizontalLayout name="sysMenuPanel" height="30" >
                  <HorizontalLayout width="300" bordersize="0,0,0,2">
                        <Button name="btnFile" text="文件" float="true" pos="0,0,50,30" bordersize="0" height="28" />
                        <Button name="btnEdit" text="编辑" float="true" pos="50,0,100,30"  bordersize="0" height="28"/>
                        <Button name="btnView" text="视图" float="true" pos="100,0,150,30"  bordersize="0" height="28"/>
                        <Button name="btnProject" text="项目" float="true" pos="150,0,200,30" bordersize="0" height="28"/>
                  </HorizontalLayout>
                  <HorizontalLayout bordersize="0,0,0,2">
                        <Label name="lbReader" text="读卡器:" float="true" pos="0,0,60,28"/>
                        <Combo name="comboReaderList" float="true" pos="70,3,370,28" droptype="droplist" width="300" height="22" fillpartwidth="20" normalimage="Icon/down_arrow1_8.png"/>
                  </HorizontalLayout>
            </HorizontalLayout>
            <!--客户区-->
            <HorizontalLayout name="clientContainer" maxwidth="1200" maxheight="700">
                  <HorizontalLayout name="treeViewMenuContainer" sepwidth="5" sepimm="true" minwidth="50" maxwidth="300" width="200">
                        <TreeViewNav name="treeViewNav"/>
                  </HorizontalLayout>
                  <VerticalLayout>
                        <TabLayout name="mainPanelContainer" selectedid="0" bordersize="2,0,0,2">
                              <ModifyKmcUI />
                              <PersonalizationUI />
                        </TabLayout>
                        <VerticalLayout name="outputPanelContainer" bordersize="2,0,0,0"  minheight="50" height="200" sepheight="-5" sepimm="true"/>
                  </VerticalLayout>
            </HorizontalLayout>
      </VerticalLayout>
</Window>