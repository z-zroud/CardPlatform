<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="1100,700" caption="0,0,0,30" roundcorner="5,5,5,5" sizebox="4,4,4,4" mininfo="600,320">
      <Font shared="true" id="0" name="幼圆" size="12" default="true" />
	<Font shared="true" id="1" name="幼圆" size="14" default="true" />
	<Font shared="true" id="2" name="微软雅黑" size="18" />	
	<Font shared="true" id="3" name="微软雅黑" size="20"/>
      <Default shared="true" name="Button" value="textcolor=&quot;#FFFFFFFF&quot; bkcolor=&quot;#FF2D2D30&quot;"/>
      <Default shared="true" name="Combo" value="bkcolor=&quot;#FF9E9E9E&quot; droptype=&quot;droplist&quot;"/>
      <VerticalLayout name="root" bkcolor="#FFFFFFFF">
            <!--标题栏-->
            <HorizontalLayout name="1" height="30" bkcolor="#FF2D2D30">
                  <HorizontalLayout name="2">
                        <Control width="20"/>
                        <Label name="appLogo" padding="0,5,5,0" bkimage="file='Icon/logo.png' source='1,4,23,20'" width="22" height="16"/>
                        <Label name="appTitle" inset="5,0,0,0" textcolor="#FFF2F2F2" text="金邦达检查制卡平台" font="0"/>
                  </HorizontalLayout>
                  <HorizontalLayout inset="0,2,0,0">
                        <HorizontalLayout>
                        </HorizontalLayout>
                        <HorizontalLayout width="120">
                              <Button float="true" pos="0,0,40,30"  name="minbtn" bordersize="0"  normalimage="Icon/min.png" />
                              <Button float="true" pos="40,0,80,30"  name="restorebtn" visible="false" bordersize="0"  normalimage="Icon/restore_10.png" />
                              <Button float="true" pos="40,0,80,30"  name="maxbtn" bordersize="0" enabled="false"  normalimage="Icon/max_10.png" />
                              <Button float="true" pos="80,0,120,30" name="closebtn" bordersize="0" normalimage="Icon/close_10.png" />
                        </HorizontalLayout>
                  </HorizontalLayout>
		</HorizontalLayout>
            <!--菜单栏-->
            <HorizontalLayout name="sysMenuPanel" height="30" bkcolor="#FF2D2D30">
                  <HorizontalLayout width="300" bordersize="0,0,0,2">
                        <Button name="btnFile" text="文件" float="true" pos="0,0,50,30" bordersize="0" height="28" />
                        <Button name="btnEdit" text="终端" float="true" pos="50,0,100,30"  bordersize="0" height="28"/>
                        <Button name="btnView" text="视图" float="true" pos="100,0,150,30"  bordersize="0" height="28"/>
                        <Button name="btnProject" text="项目" float="true" pos="150,0,200,30" bordersize="0" height="28"/>
                  </HorizontalLayout>
                  <HorizontalLayout bordersize="0,0,0,2">
                        <Label name="lbReader" text="读卡器:" textcolor="#FFF2F2F2" float="true" pos="0,0,60,28"/>
                        <Combo name="comboReaderList" float="true" pos="70,3,370,28"  width="300" height="22" fillpartwidth="20" normalimage="Icon/down_arrow1_8.png"/>
                  </HorizontalLayout>
            </HorizontalLayout>
            <!--客户区-->
            <HorizontalLayout name="clientContainer">
                  <!--左侧树形导航区域-->
                  <HorizontalLayout name="treeViewMenuContainer" sepwidth="5" sepimm="true" minwidth="50" maxwidth="300" width="200">
                        <TreeViewNav name="treeViewNav" bkcolor="#FF7F7F7F"/>
                  </HorizontalLayout>
                  <VerticalLayout>
                        <!--中间主区域-->
                        <TabLayout name="mainPanelContainer" bkcolor="#FF7F7F7F" selectedid="1" bordersize="2,0,0,2">
                              <YLConfigUI name="ylConfigPanel" />
                              <YLConverterUI name="ylConverterPanel"/>
                              <HTConverterUI name="htConverterPanel"/>
                              <GEConverterUI name="geConverterPanel"/>
                              <PersonalizationUI />
                              <CardCheckUI />
                              <ModifyKmcUI />
                        </TabLayout>
                        <VerticalLayout name="outputPanelContainer" bkcolor="#FF7F7F7F" bordersize="2,0,0,0"  minheight="50" height="200" sepheight="-5" sepimm="true">
                              <!--输出、错误选项控件-->
                              <HorizontalLayout height="20">
                                    <Option name="optOutput" float="true" pos="00,0,50,20" text="Output" bkcolor="#FF7F7F7F" selectedbkcolor="#FF007ACC" group="grpOutput" selected="true"/>
                                    <Option name="optError" float="true" pos="50,0,100,20" text="Error" bkcolor="#FF7F7F7F" selectedbkcolor="#FF007ACC" group="grpOutput"/>
                              </HorizontalLayout>
                              <HorizontalLayout height="3">
                                    <Control bordersize="3" bordercolor="#FF007ACC" /> 
                              </HorizontalLayout> 
                              <!--输出窗口-->
                              <TabLayout name="tabOutputPanel">
                                    <HorizontalLayout>
                                          <RichEdit name="richOutput" text="output" bkcolor="#FF9E9E9E" vscrollbar="true" autovscroll="true" hscrollbar="true" />
                                    </HorizontalLayout>
                                    <HorizontalLayout>
                                          <RichEdit name="richError" text="error" bkcolor="#FF9E9E9E" vscrollbar="true" autovscroll="true" hscrollbar="true" />
                                    </HorizontalLayout>
                              </TabLayout>
                        </VerticalLayout>
                  </VerticalLayout>
            </HorizontalLayout>
      </VerticalLayout>
</Window>