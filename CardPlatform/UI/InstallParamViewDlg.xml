<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="800,500" caption="0,0,0,30" roundcorner="5,5,5,5" sizebox="4,4,4,4" mininfo="600,320">
      <Font shared="true" id="0" name="幼圆" size="12" default="true" />
      <Font shared="true" id="1" name="幼圆" size="14" default="true" />
      <Font shared="true" id="2" name="微软雅黑" size="18" />
      <Font shared="true" id="3" name="微软雅黑" size="20"/>
      <Default shared="true" name="Button" value="textcolor=&quot;#FFFFFFFF&quot; bkcolor=&quot;#FF2D2D30&quot;"/>
      <Default shared="true" name="Combo" value="bkcolor=&quot;#FF3F3F46&quot; droptype=&quot;droplist&quot;"/>
      <VerticalLayout name="root" bkcolor="#FFFFFFFF">
            <!--标题栏-->
            <HorizontalLayout name="1" height="30" bkcolor="#FF2D2D30">
                  <HorizontalLayout name="2">
                        <Control width="20"/>
                        <Label name="installParamLogo" padding="0,5,5,0" bkimage="file='Icon/logo.png' source='1,4,23,20'" width="22" height="16"/>
                        <Label name="installParamTitle" inset="5,0,0,0" textcolor="#FFF2F2F2" font="0"/>
                  </HorizontalLayout>
                  <HorizontalLayout>
                        <HorizontalLayout>
                        </HorizontalLayout>
                        <HorizontalLayout width="40">
                              <Button float="true" pos="0,0,30,20" name="closebtn" bordersize="0" normalimage="Icon/close_10.png" />
                        </HorizontalLayout>
                  </HorizontalLayout>
            </HorizontalLayout>
            <!--对话框内容-->
            <VerticalLayout bkcolor="#FF7F7F7F">
                  <Control height="50" />
                  <!--表头-->
                  <HorizontalLayout height="50" bkcolor="#FF9E9E9E">
                        <Label name="blank" float="true" pos="0,0,100,30" align="center" />
                        <Label text="PSE" float="true" pos="100,0,320,30" align="center" />
                        <Label text="PPSE" float="true" pos="320,0,540,30" align="center" />
                        <Label text="Application" float="true" pos="540,0,760,30" align="center" />
                  </HorizontalLayout>
                  <!--Package AID-->
                  <HorizontalLayout name="packagePanel" height="50" bkcolor="#FF9E9E9E">
                        <Label text="Package AID" float="true" pos="0,0,100,30" align="right" />
                  </HorizontalLayout>
                  <!--Applet AID-->
                  <HorizontalLayout name="appletPanel" height="50" bkcolor="#FF9E9E9E">
                        <Label text="Applet AID" float="true" pos="0,0,100,30" align="right" />
                  </HorizontalLayout>
                  <HorizontalLayout name="varPanel" height="50" bkcolor="#FF9E9E9E">
                        <Label text="Applet AID" float="true" pos="0,0,100,30" align="right" />
                  </HorizontalLayout>
                  <!--App Permissions-->
                  <HorizontalLayout name="permissionPanel" height="50" bkcolor="#FF9E9E9E">
                        <Label text="App Permission" float="true" pos="0,0,100,30" align="right" />
                  </HorizontalLayout>
                  <!--Install Param-->
                  <HorizontalLayout name="paramPanel" height="50" bkcolor="#FF9E9E9E">
                        <Label text="Install Param" float="true" pos="0,0,100,30" align="right" />
                  </HorizontalLayout>
                  <!--Token-->
                  <HorizontalLayout name="tokenPanel" height="50" bkcolor="#FF9E9E9E">
                        <Label text="Token" float="true" pos="0,0,100,30" align="right" />
                  </HorizontalLayout>
            </VerticalLayout>
      </VerticalLayout>
</Window>

